  },\n        incrementOdd(){\n          if((this.$store.state.sum % 2)!=0){\n            this.$store.dispatch('jia',Number(this.n));\n          }\n        },\n        incrementWait(){\n          setTimeout(()=>{\n            this.$store.dispatch('jia',Number(this.n));\n          },500);\n        }\n    },\n    mounted(){\n      console.log(this)\n    }\n}\n</script>"]}]}