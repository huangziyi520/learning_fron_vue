n// 准备mutations-用于操作数据(state)\nconst mutations={\n    JIA(miniState,value){\n        miniState.sum+=value;\n    },\n    JIAN(miniState,value){\n        miniState.sum-=value;\n    }\n}\n// 准备state-用于存储数据\nconst state={\n    sum:1\n}\n//将state中的数据进行加工后显示\nconst getters={\n    bigSum(state){\n        return state.sum*10\n    }\n}\n// 创建并暴露store\nVue.use(Vuex)\nexport default new Vuex.Store({\n    actions,mutations,state,getters\n})"]}]}